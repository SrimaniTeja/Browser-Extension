{
    "name": "smth",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "tabs",
        "webRequest",
        "webNavigation",
        "management",
        "scripting"
    ],"host_permissions": [
        "<all_urls>"
    ],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "service.js",
      "type": "module"
    }
}